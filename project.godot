; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "FiniteStateMachine",
"language": "GDScript",
"path": "res://addons/finite_state_machine/scripts/finite_state_machine.gd"
}, {
"base": "Node",
"class": "StateMachineState",
"language": "GDScript",
"path": "res://addons/finite_state_machine/scripts/state_machine_state.gd"
} ]
_global_script_class_icons={
"FiniteStateMachine": "res://addons/finite_state_machine/icons/finite_state_machine.svg",
"StateMachineState": "res://addons/finite_state_machine/icons/state_machine_state.svg"
}

[application]

config/name="Finite State Machine"
run/main_scene="res://example/scenes/example_scene.tscn"
config/icon="res://icon.svg"

[editor_plugins]

enabled=PoolStringArray( "res://addons/finite_state_machine/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true
2d/default_gravity=980
